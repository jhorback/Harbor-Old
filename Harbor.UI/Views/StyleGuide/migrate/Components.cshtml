@section head {
	@Styles.Render("~/content/site/ext/pager.css")
}

<div class="page-content">
	

	
	
	
	<h2>Pager</h2>
	<div class="outline example">
		
		<div class="pager">
			<ul>
				<li class="disabled" title="first page"><span class="pager-symbol">&laquo;</span></li>
				<li class="disabled" title="previous page"><span class="pager-symbol">&lsaquo;</span></li>
				<li class="selected"><a href="#">1</a></li>
				<li><a href="#">2</a></li>
				<li><a href="#">3</a></li>
				<li><a href="#">4</a></li>
				<li><a href="#">5</a></li>
				<li><a href="#" title="next page" class="pager-symbol">&rsaquo;</a></li>
				<li><a href="#" title="last page" class="pager-symbol">&raquo;</a></li>
			</ul>
		</div>

	</div>
	<pre class="prettyprint">
Add markup and examples
</pre>
</div>


