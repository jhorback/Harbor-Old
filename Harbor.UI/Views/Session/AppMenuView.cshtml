<div data-templatefor="appMenuView" class="appmenu">
	<div class="toolbar">
		<div>
			<span class="icon-close float-left" data-event="closeMenu"></span>
			<a href="~/User/SignOut" data-event="signOut" class="appmenu-logout button flat attn float-right">Sign Out</a>
		</div>
		<div>
			<h2 data-bind="text: currentUser.usersDisplayName"></h2>
		</div>
	</div>
	<hr />
	<div class="menulist">
		<ul class="appmenu-parentlist">
			<li data-collection="parentItems" data-bind="class: className">
				<span data-event="selectMenu">
					<strong data-bind="text:text"></strong>
				</span>
			</li>
		</ul>
		<ul class="appmenu-childlist">
			<li data-collection="childItems" data-bind="class: className">
				<a data-bind="href: url" data-event="selectMenuItem" class="clearfix">
					<span class="float-left" data-bind="text:text"></span>
					<span class="float-right icon-next" data-bind="showif:isMenu"></span>
				</a>
			</li>
		</ul>
	</div>
</div>