@model Common.UI.Models.JsPackage
@{
	ViewBag.Title = Model.Name + " - Tests - Common Controls";
}
@section Head{
	<link rel="stylesheet" href="~/Content/qunit.css"/>
	<script src="~/Scripts/qunit.js"></script>
	@Html.Partial("_JsPackageScripts", Model)
}


<div id="qunit">
	<h1 id="qunit-header">@Model.JsName</h1>
	
	<a class="button loud" href="@Url.Action("Package", "Packages", new {  name = Model.Name})" style="float:right;position:relative;top:-1.3em;">Back to @Model.JsName package</a>
	<h2 id="qunit-banner"></h2>
	<div id="qunit-testrunner-toolbar"></div>
	<h2 id="qunit-userAgent"></h2>
	<ol id="qunit-tests"></ol>
	<div id="qunit-fixture">test markup, will be hidden</div>
</div>
